{
  "name": "assignment-problem-frontend",
  "version": "1.0.0",
  "description": "Create new project React Typescript in command line:\r npx create-react-app my-app --template typescript",
  "main": "dist/main.js",
  "scripts": {
    "build:react": "webpack --mode development --config webpack.react.config.js",
    "build": "webpack --mode development",
    "start": "electron ./dist/main.js",
    "format": "prettier --config .prettierrc --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "package-win": "electron-packager . electron-demo --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron-demo\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkl-314/assignment-problem-frontend.git"
  },
  "keywords": [],
  "author": "Ming Kim Low",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mkl-314/assignment-problem-frontend/issues"
  },
  "homepage": "https://github.com/mkl-314/assignment-problem-frontend#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-arrow-functions": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^6.2.0",
    "electron": "^13.1.9",
    "electron-cgi": "^1.0.6",
    "electron-packager": "^15.3.0",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^6.0.1",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "antd": "^4.16.11",
    "honeycomb-web-toolkit": "git+https://github.com/red-gate/honeycomb-web-toolkit.git",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-excel-renderer": "^1.1.0",
    "react-promise-tracker": "^2.1.0",
    "xlsx": "^0.17.0"
  }
}
